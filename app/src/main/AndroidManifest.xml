<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.application.ravenrankboost">

    <uses-permission android:name="android.permission.STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RavenRankBoost">
        <activity android:name=".View.PrivacyNoticeActivity"></activity>
        <activity android:name=".View.TermsOfServiceActivity" />
        <activity android:name=".View.AdminOrder.AdminOrderActivity" />
        <activity android:name=".View.ContactCenterActivity" />
        <activity android:name=".View.ProfileActivity" />
        <activity android:name=".View.Notifications.NotificationActivity" />
        <activity android:name=".View.OrderDetails.OrderDetailActivity" />
        <activity android:name=".View.ListOrder.ListOrderActivity" />
        <activity android:name=".View.NewOrder.NewOrderActivity" />
        <activity android:name=".View.Dashboard.DashboardActivity" />
        <activity android:name=".View.SignUpActivity" />
        <activity android:name=".View.SignInActivity" />
        <activity android:name=".View.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>